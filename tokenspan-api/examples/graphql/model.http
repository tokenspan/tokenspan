### Update model
GRAPHQL {{host}}/graphql
Authorization: Bearer {{token}}

mutation UpdateModel($updateModelId: UUID!, $input: ModelUpdateInput!) {
  updateModel(id: $updateModelId, input: $input) {
    id
    name
    description
    slug
    context
    inputPricing {
      price
      tokens
      currency
    }
    outputPricing {
      price
      tokens
      currency
    }
    trainingAt
    providerId
    createdAt
    updatedAt
  }
}

