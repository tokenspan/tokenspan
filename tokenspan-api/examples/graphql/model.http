### Get models
GRAPHQL {{host}}/graphql
Authorization: Bearer {{token}}

query Models {
  models {
    nodes {
      id
      name
      description
      slug
      context
      inputPricing {
        price
        tokens
        currency
      }
      outputPricing {
        price
        tokens
        currency
      }
      trainingAt
      providerId
      createdAt
      updatedAt
    }
    totalNodes
    pageInfo {
      hasPreviousPage
      hasNextPage
      startCursor
      endCursor
    }
  }
}

### Get model by id
GRAPHQL {{host}}/graphql
Authorization: Bearer {{token}}

query Model($modelId: UUID!) {
  model(id: $modelId) {
    id
    name
    description
    slug
    context
    inputPricing {
      price
      tokens
      currency
    }
    outputPricing {
      price
      tokens
      currency
