### Create task
GRAPHQL {{host}}/graphql
Authorization: Bearer {{token}}

mutation CreateTask($input: TaskCreateInput!) {
  createTask(input: $input) {
    id
    name
    slug
    createdAt
    updatedAt
    version {
      id
      version
      releaseNote
      description
      document
      status
      taskId
      createdAt
      updatedAt
    }
  }
}

{
