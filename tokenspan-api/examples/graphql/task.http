  }
}

### Get tasks
GRAPHQL {{host}}/graphql
Authorization: Bearer {{token}}

query Task {
  tasks {
    nodes {
      id
      name
      slug
      createdAt
      updatedAt
      version {
        id
        version
        semver
        releaseNote
        description
        document
        status
        taskId
        createdAt
        updatedAt
        parameters {
          id
          name
          temperature
          maxTokens
          stopSequences
          topP
          frequencyPenalty
          presencePenalty
          extra
          modelId
          createdAt
          updatedAt
          model {
            id
            name
            description
            slug
            context
            inputPricing {
              price
              tokens
              currency
            }
            outputPricing {
              price
              tokens
              currency
            }
            trainingAt
            providerId
            createdAt
            updatedAt
          }
        }
      }
    }
    totalNodes
    pageInfo {
      hasPreviousPage
      hasNextPage
      startCursor
      endCursor
    }
  }
}